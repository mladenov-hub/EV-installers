{
    "name": "ev-installers-app",
    "framework": "nextjs",
    "buildCommand": "next build",
    "devCommand": "next dev",
    "installCommand": "npm install",
    "crons": [
        {
            "path": "/api/cron/promoter",
            "schedule": "0 10 * * *"
        },
        {
            "path": "/api/cron/operator",
            "schedule": "0 3 * * 0"
        },
        {
            "path": "/api/cron/google-places",
            "schedule": "0 2 * * *"
        }
    ]
}